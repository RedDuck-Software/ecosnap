{
  "name": "vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" vite build",
    "format": "yarn format:prettier && yarn format:es",
    "format:prettier": "prettier --write \"src/**/*.{ts,tsx,d.ts}\"",
    "format:es": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint": "yarn lint:prettier && yarn lint:es",
    "lint:prettier": "prettier --check \"src/**/*.ts\"",
    "lint:es": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.93.0",
    "@tanstack/react-query": "^5.45.0",
    "bs58": "^5.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.395.0",
    "next": "14.2.4",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@gc/eslint-config": "*",
    "@gc/typescript-config": "*",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "autoprefixer": "^10.4.19",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.1",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
